{
  "name": "propsku-backend",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env -- tsx src/index.ts",
    "build": "tsc && mkdir -p ./dist/services && cp -r ./src/services/* ./dist/services/",
    "start": "NODE_ENV=production node --expose-gc dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:init": "tsx src/cli/init-db.ts",
    "db:migrate": "node migrate.js",
    "create:portfolio": "tsx src/cli/create-portfolio.ts"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.4",
    "archiver": "^7.0.1",
    "bcrypt": "^5.1.1",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "memorystore": "^1.6.7",
    "nanoid": "^5.0.5",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.16.0",
    "pg": "^8.11.3",
    "vite": "^5.1.4",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "20.16.11",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/pdfkit": "^0.12.12",
    "@types/pg": "^8.10.9",
    "drizzle-kit": "^0.30.4",
    "esbuild": "^0.25.0",
    "tsx": "^4.19.1",
    "typescript": "5.6.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
